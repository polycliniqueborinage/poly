/*basic*/
html { 
	min-width: 600px; 
}
body, div, td, th, h2, h3, h4 {  
    font-family: arial, sans-serif;
    font-size: 11px;
	color: #333;
}  
body {
	background-color: #fff;
	margin: 0; 
	padding: 0;
}
a:visited, a:active, a { 
  text-decoration: none;
  cursor: pointer;
}
img {
  border: none;
  margin: 0;
  padding: 0;
}
h1,h2,h3,h4,h5 {
	font-family: "Trebuchet MS", verdana;
	color: #101010;
	margin: 0;
}

/*agenda*/
h1 { 
    color:#333366; 
    font-size: 18px; 
    padding: 5px 0px 0px 10px; 
}
h2 {
    font-size: 13px;
	font-weight: normal;
	padding: 0;
	border: 0;
}
#topcalendrier { 
border-top: solid 1px #B5CBE7;
margin: 0; 
padding: 0; 
height: 50px; 
background: #B5CBE7;
}
#left { 
position: absolute; 
top: 77px; 
background:transparent;
left: 0; 
width: 203px;  
margin: 0; 
}
#middle { 
position: relative; 
top: -19px; 
min-width: 350px; 
background-color: transparent;	
margin: 0 5px 10px 210px;  
padding: 0 0 5px 0; 
}
#middleleft { 
position: relative; 
top: -19px; 
min-width: 350px; 
background-color: transparent;	
margin: 0 5px 10px 10px;  
padding: 0 0 5px 0; 
}

/* Footer
   =========================== */
#footer {
margin: 10px 5px;
color: #999;
text-align: center;
color: #aaa;
}
/*#single-layout #footer {
margin-top: 35px;
border-top: solid 1px #eee;
padding-top: 5px;
margin-left: 15px;
}*/
#footer p {
margin: 0;
font-size: 10px;
color: #aaa;
font-family: verdana;
line-height: 10px;
}
/*
#footer a {
font-family: verdana;
text-decoration: underline;
font-size: 10px;
}
*/

/* DropDOwn
   =========================== */
#listspecialite, #listmedecin, #listduree,#impression {
background: transparent;
position: absolute; 
top: 30px;
padding: 0;
width: 100%;
}
#listspecialite {
left:250px;
}
#listmedecin {
left:550px;
}
#listduree {
left:730px;
}
#impression {
left:870px;
}

/* Hide Sidebars
   =========================== */
.hide-sidebars #top { 
background: #B5CBE7 bottom right no-repeat;
}
.hide-sidebars #calendarSideBar { 
display: none;
}
.hide-sidebars #left {
left: -193px;
}
.hide-sidebars #middle { 
margin-left: 15px;
}
.hide-sidebars #calendar_tab_panel .calendarViewPane {
margin-right: 5px;
}

/* Misc
   =========================== */
.inlineForm {
	padding: 4px 2px;
	font-size: 10px;
	background-color: transparent;
	text-align: right;
}
.inlineForm input { 
	width: 95%;
	font-size: 11px;
	padding: 1px;
	color: #aaa;
}
div#system_message_panel .notice {
color: #f00;
}
div#system_message_panel .error {
color: #f00;
}
.NoSelect {
        -moz-user-select:none;
}
.clearFix {
clear: both;
}

/*tabs menu tableau*/
#secondmain {
	margin-bottom: 0px;
	margin-left: 2px;
	margin-right: 2px;
	margin-top: 0px;
	padding-top: 20px;
	padding-bottom: 20px;
	padding-left: 1px;
	border: 1px solid #6C94C0;
	padding-bottom: 5px;
	background-color: #c5dbf7;
}
#main {
	border: 1px solid #6C94C0;
	padding-bottom: 5px;
}

#secondheader{
	position: relative;
	width: 100%;
	height: 2.1em;
	background-color: transparent;
}
#header {
	position: relative;
	width: 100%;
	height: 2.1em;
	background-color: transparent;
}

/* Top Row */
#header ul#primary_tabs {
	margin: 0;
	padding: 0;
	position: absolute;
	bottom: -1px;
}

#secondheader ul#sec_primary_tabs{
	margin-bottom: 0px;
	margin-left: 2px;
	margin-right: 2px;
	margin-top: 0px;
	padding: 0;
	position: absolute;
	bottom: -1px;
}

* html #secondheader ul#sec_primary_tabs ,
* html #header ul#primary_tabs {
    bottom: -2px;
}

#secondheader ul#sec_primary_tabs li ,
#header ul#primary_tabs li {
    font-size: 11px;
    float: left;
    position: relative;
	display: inline;
	border: 0;
	font-family: "Trebuchet MS", verdana;
	color: #101010;
	padding: 0;
	margin: 3px 0 0 0;
}

#secondheader ul#sec_primary_tabs a,
#secondheader ul#sec_primary_tabs li.current a ,
#header ul#primary_tabs a,
#header ul#primary_tabs li.current a {
	margin: 1px 2px 0 0;
	padding: 1px 18px 1px 3px;
	float: left;
	text-align: center;
}	

#secondheader ul#sec_primary_tabs li.current a,
#secondheader ul#sec_primary_tabs li.current a:hover ,
#header ul#primary_tabs li.current a,
#header ul#primary_tabs li.current a:hover {
	border: 1px solid #6C94C0;
	border-bottom: none;
	background: #C5DBF7;
	padding-bottom: 3px;
	margin-top: 1px;
	text-decoration: none;
	color: #000;
	cursor: default;
}

#secondheader ul#sec_primary_tabs a ,
#header ul#primary_tabs a {
	background: #f0f0ff; 
	border: 1px solid #6C94C0;
	border-bottom: none;
	margin-top: 2px;
}

#secondheader ul#sec_primary_tabs a:hover ,
#header ul#primary_tabs a:hover {
	background: #c5f7c5;
}

/* Static Tabs */
#secondheader ul#sec_primary_tabs li.letter ,
#secondheader ul#sec_primary_tabs li.nodelete ,
#header ul#primary_tabs li.nodelete {
        position: static;
        font-size: 13px;
        padding: 0;
        margin: 0;
}

#secondheader ul#sec_primary_tabs li.nodelete a,
#secondheader ul#sec_primary_tabs li.current a.nodelete,
#secondheader ul#sec_primary_tabs li.current a.nodelete:hover {
        padding: 2px 0 1px 0;
        margin: 2px 2px 0 0;
        width: 4.3em;
        float: left;
        text-align: center;
}       

#secondheader ul#sec_primary_tabs li.letter a,
#secondheader ul#sec_primary_tabs li.letter.current a.nodelete,
#secondheader ul#sec_primary_tabs li.letter.current a.nodelete:hover {
        padding: 2px 0 1px 0;
        margin: 2px 2px 0 0;
        width: 1.5em;
        float: left;
        text-align: center;
}       

#header ul#primary_tabs li.nodelete a,
#header ul#primary_tabs li.current a.nodelete,
#header ul#primary_tabs li.current a.nodelete:hover {
        padding: 2px 0 1px 0;
        margin: 2px 2px 0 0;
        width: 6em;
        float: left;
        text-align: center;
}       

#secondheader ul#sec_primary_tabs li.letter.current a.nodelete,
#secondheader ul#sec_primary_tabs li.letter.current a.nodelete:hover ,
#secondheader ul#sec_primary_tabs li.current a.nodelete,
#secondheader ul#sec_primary_tabs li.current a.nodelete:hover ,
#header ul#primary_tabs li.current a.nodelete,
#header ul#primary_tabs li.current a.nodelete:hover {
        padding-bottom: 2px;
}

/* Secondary Tabs */

#main .secondary-tabs {
padding: 2px 2px 2px 2px;
font-size: 10px;
height: 15px;
background: #C5DBF7;
font-family: verdana, arial, sans-serif;
}

#main .secondary-tabs ul {
margin: 0;
padding: 0;
list-style: none;
}

#main .secondary-tabs ul li {
display: inline;
}

#main .secondary-tabs ul li.current a {
background: #fff;
padding-bottom: 3px;
color: #000;
cursor: default;
}

#main .secondary-tabs a,
#main .secondary-tabs span {
padding: 1px 10px;
}

#main .secondary-tabs span {
background: #fff;
padding-bottom: 5px;
}

#main .secondary-tabs a:hover {
	background: #c5f7c5;
	padding-bottom: 3px;
}


/*sidebare*/
/* LEFT PART
   =========================== */
#left {
position:fixed;
top:130px;
}

#left-drawer td.drawer-content {
width: 190px;
padding-right: 2px;
background-color:#b5cbe7;
}

#left-drawer td.drawer-handle {
width: 10px;
background: #5A94CE url(../images/arrow-right.gif) 3px 50% no-repeat;
cursor: pointer;
}

.hide-sidebars #left-drawer td.drawer-handle {
background: #5A94CE url(../images/arrow-left.gif) 3px 50% no-repeat;
}

#left-drawer td.drawer-handle .top-corner {
height: 4px;
width: 4px;
position: absolute;
background: url(../images/corner-top-right.gif) top right no-repeat;
top: 0px;
right: 0;
}

#left-drawer td.drawer-handle .bottom-corner {
height: 4px;
width: 4px;
position: absolute;
background: url(../images/corner-bottom-right.gif) bottom right no-repeat;
bottom: 0;
right: 0;
}

