.cellSelection #selection-start-time,
.cellSelectionEnd #selection-start-time,
.cellSelectionEndNew #selection-start-time,
.cellSelection #selection-end-time,
#drag-end-time,
.cellSelectionEnd #selection-end-time,
.cellSelectionEndNew #selection-end-time,
#drag-end-time {
background: transparent;
font: normal 10px arial;
-moz-user-select:none;
} 

.cellSelection #selection-start-time,
.cellSelection #selection-end-time {
cursor: default;
color: #666;
}

.cellSelectionEndNew #selection-start-time,
.cellSelectionEndNew #selection-end-time,
.cellSelectionEnd #selection-start-time,
.cellSelectionEnd #selection-end-time {
cursor: default;
color:#000000;
}
       
.cellSelection #selection-start-time,
.cellSelectionEndNew #selection-start-time,
.cellSelectionEnd #selection-start-time {
position: absolute;
top: -1px;
right: 1px;
}

.cellSelection #selection-end-time,
#drag-end-time ,
.cellSelectionEndNew #selection-end-time,
#drag-end-time,
.cellSelectionEnd #selection-end-time,
#drag-end-time {
position: absolute;
bottom: -1px;
right: 1px;
} 

.cellSelectionEndNew #selection-remove,
.cellSelectionEnd #selection-remove {
position: absolute;
top: 1px;
left: 2px;
border:none;
height:18px;
}

.cellSelectionEnd #selection-info,
.cellSelectionEndNew #selection-info {
position: absolute;
top: 1px;
left: 22px;
border:none;
height:18px;
}

.cellSelectionEnd #selection-print,
.cellSelectionEndNew #selection-print {
position: absolute;
top: 1px;
left: 42px;
border:none;
height:18px;
}

.cellSelectionEnd .inplaceeditor-empty-big,
.cellSelectionEnd .inplaceeditor-empty-small,
.cellSelectionEndNew .inplaceeditor-empty-big,
.cellSelectionEndNew .inplaceeditor-empty-small {
position: absolute;
top: 1px;
left: 62px;
right: 27px;
font-weight:bold;
}

.cellSelectionEnd .inplaceeditor-empty-small,
.cellSelectionEndNew .inplaceeditor-empty-small {
font-size:0.9em;
}

.cellSelectionEnd .inplaceeditor-empty-big ,
.cellSelectionEndNew .inplaceeditor-empty-big {
