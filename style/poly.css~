    float:left;
    display:inline; /* Prevent margin doubling in IE */
    padding:4px 12px;
    border-width:0 1px 0 0;
}

div.yuimenu h6 {
    float:none;
    display:block;
    border-width:1px 0 0 0;
    padding:5px 10px 0 10px;
}

/* Matches the UL inside a Menu or MenuBar instance */

div.yuimenubar ul {
    list-style-type:none;
    margin:0;
    padding:0;
}

div.yuimenu ul {
    list-style-type:none;
